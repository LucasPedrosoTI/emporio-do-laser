{
  "name": "emporio-do-laser",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www --inspect",
    "cm": "npx sequelize migration:create",
    "mg": "npx sequelize-cli model:generate",
    "dbm": "npx sequelize-cli db:migrate",
    "dbs": "npx sequelize-cli seed:generate --name ",
    "dbsa": "npx sequelize-cli db:seed:all",
    "dbso": "npx sequelize-cli db:seed --seed ",
    "dbmu": "npx sequelize-cli db:migrate:undo",
    "dbmuall": "npx sequelize-cli db:migrate:undo:all",
    "dbsuall": "npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "mailgun-js": "^0.22.0",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "mysql2": "^2.3.0",
    "node-correios": "^3.0.2",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  }
}
