<%- include('../components/head', {title: 'Minha Conta: Dados'}) %> <%- include('../components/navbar') %>

<div class="corpo">
  <section>
    <%- include('../components/minha-conta/lateral-menu') %>

    <div class="email">
      <ul class="email-title">
        <strong>Meus Dados</strong>
      </ul>
      <div class="componentes">
        <div class="caixa1">
          <p class="subtitulos espaco">Nome Completo</p>
          <p class="subtitulos espaco">CPF</p>
        </div>

        <div class="caixa2">
          <input type="text" name="nome" class="caixa-senha magin-senha" />
          <input type="text" name="cpf" class="caixa-senha" onkeypress="$(this).mask('000.000.000-00')" />
        </div>

        <div class="caixa1">
          <p class="subtitulos espaco">Telefone Principal</p>
        </div>

        <div class="caixa2">
          <input type="tel" name="telefone" class="caixa-senha" />
          <button type="submit" class="btn-gravar-dados">Salvar Dados</button>
        </div>
      </div>
    </div>
  </section>
</div>

<%- include('../components/foot') %>
